{
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/**/*.py": {
            "runtime": "python3.9",
            "maxDuration": 60
        }
    }
}
